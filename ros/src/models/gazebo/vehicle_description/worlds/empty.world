<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- set max step size to 0.01 -->
    <plugin name="set_step_size" filename="libset_step_size.so"/>
  </world>
</sdf>
